{
  "name": "sa-insights-lite",
  "homepage": "https://themewagon.github.io/Sa-Insights/",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "vite --mode localhost",
    "dev:development": "vite --mode development",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "build:local": "vite build --mode localhost",
    "build:development": " NODE_OPTIONS=--max-old-space-size=8192 vite build --mode development",
    "build:production": " NODE_OPTIONS=--max-old-space-size=8192 vite build --mode production",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "serve -s dist -l tcp://0.0.0.0:9398",
    "start:local": "npm run build:local && serve -s dist -l tcp://localhost:9398",
    "start:dev": "npm run build:development && serve -s dist -l tcp://0.0.0.0:9398"
  },
  "dependencies": {
    "@arcgis/core": "^4.33.13",
    "@arcgis/map-components": "^4.33.19",
    "@babel/runtime": "^7.28.4",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.17.1",
    "@mui/lab": "5.0.0-alpha.133",
    "@mui/material": "^5.15.13",
    "@mui/system": "^5.14.11",
    "@mui/x-tree-view": "^8.11.2",
    "@svgr/rollup": "^8.1.0",
    "@tabler/icons-react": "^2.35.0",
    "antd": "^5.27.2",
    "apexcharts": "^5.3.5",
    "esbuild": "^0.18.20",
    "html2canvas": "^1.4.1",
    "neo4j-driver": "^5.28.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.2.0",
    "react-force-graph-2d": "^1.29.0",
    "react-helmet": "^6.1.0",
    "react-router": "^6.16.0",
    "react-router-dom": "^6.16.0",
    "react-swipeable-views-react-18-fix": "^0.14.1",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@esbuild/linux-x64": "^0.18.20",
    "@types/react": "^18.2.15",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.3.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "serve": "^14.2.5",
    "vite": "^4.4.5"
  },
  "description": "SA Insights Lite - A lightweight version of the SA Web Service with NewsRoom, Intelligence Briefings, and Event Timeline.",
  "main": "vite.config.js",
  "keywords": ["insights", "news", "timeline", "intelligence"],
  "author": "",
  "license": "ISC"
}
